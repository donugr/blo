{
  "name": "blo",
  "version": "1.1.1",
  "license": "MIT",
  "author": "donugr",
  "description": "blo is a small and fast library to generate Ethereum identicons. this is fork from https://github.com/bpierre/blo",
  "keywords": [
    "ethereum",
    "eth",
    "identicon",
    "blockies",
    "web3",
    "dapps"
  ],
  "homepage": "https://github.com/donugr/blo",
  "bugs": "https://github.com/donugr/blo/issues",
  "repository": "github:donugr/blo",
  "type": "module",
  "module": "./dist/blo.js",
  "main": "./dist/blo.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/blo.cjs",
      "import": "./dist/blo.js"
    }
  },
  "scripts": {
    "dev": "vite ./demos/react",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=16"
  }
}
